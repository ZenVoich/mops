	},
	"dfx": "0.21.0",
	"networks": {
		"staging": {
			"type": "persistent",
			"providers": ["https://icp0.io"]
		},
		"ic": {
			"type": "persistent",
			"providers": ["https://icp0.io"]
		}
	}
