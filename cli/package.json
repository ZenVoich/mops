{
	"name": "ic-mops",
	"version": "0.6.1",
	"type": "commonjs",
	"bin": {
		"mops": "dist/cli.js"
	},
	"homepage": "https://mops.one",
	"repository": {
		"type": "git",
		"url": "https://github.com/ZenVoich/mops.git"
	},
	"author": "Zen Voich <zen.voich@gmail.com>",
	"scripts": {
		"cli": "npm run build && node dist/cli.js",
		"build": "shx rm -rf ./dist && node build.mjs",
		"package": "shx rm -rf ./pkg && npm run build && pkg . -o pkg/mops",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@dfinity/agent": "^0.11.0",
		"@dfinity/candid": "^0.11.0",
		"@dfinity/identity": "^0.11.0",
		"@dfinity/principal": "^0.11.1",
		"@iarna/toml": "^2.2.5",
		"as-table": "^1.0.55",
		"chalk": "^4.1.2",
		"commander": "^9.2.0",
		"decompress": "^4.2.1",
		"del": "^6.0.0",
		"dhall-to-json-cli": "^1.7.6",
		"eslint": "^8.15.0",
		"execa": "^6.1.0",
		"get-folder-size": "^4.0.0",
		"globby": "^13.1.1",
		"got": "^12.5.3",
		"log-update": "^5.0.1",
		"minimatch": "^5.0.1",
		"ncp": "^2.0.0",
		"node-fetch": "^2.6.7",
		"pem-file": "^1.0.1",
		"prompts": "^2.4.2"
	},
	"devDependencies": {
		"@chialab/esbuild-plugin-meta-url": "^0.17.2",
		"esbuild": "^0.17.5",
		"pkg": "^5.8.0",
		"shx": "^0.3.4"
	},
	"files": [
		"dist/**/*"
	],
	"pkg": {
		"compress": "GZip",
		"assets": [
			"dist/package.json"
		],
		"targets": [
			"node18-linux",
			"node18-macos",
			"node18-win"
		]
	}
}
